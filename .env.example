# API Keys
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here
GOOGLE_API_KEY=your_google_key_here
HUGGINGFACE_API_KEY=your_huggingface_key_here

# Configuration
LOG_LEVEL=INFO
METAREASON_CONFIG_PATH=./config/default.yaml
METAREASON_ENV=development

# Database (if needed)
DATABASE_URL=sqlite:///metareason.db

# Cache Settings
CACHE_DIR=./.cache
CACHE_TTL=3600

# Evaluation Settings
DEFAULT_TIMEOUT=300
MAX_CONCURRENT_REQUESTS=10
RETRY_ATTEMPTS=3
RETRY_DELAY=1.0

# Output Settings
REPORT_FORMAT=html
REPORT_OUTPUT_DIR=./reports
